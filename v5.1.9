local function UYNCL_fake_script() -- WUTG.handletweeningimage 
	local script = Instance.new('LocalScript', WUTG)

	local TweenService = game:GetService("TweenService")
	
	local function tweean(object)
		local rotationInfo = TweenInfo.new(2, Enum.EasingStyle.Bounce, Enum.EasingDirection.InOut)
		local position = UDim2.new(0.327, 0, 0.144, 0)
	
		local rotationTween = TweenService:Create(object, rotationInfo, {Position = position})
	
		rotationTween:Play()
	end
	
	wait(2)
	tweean(script.Parent.wutglogo)
	wait(3)
	script.Parent.wutglogo:Destroy()
	script.Parent.oben.Visible = true
end
coroutine.wrap(UYNCL_fake_script)()
