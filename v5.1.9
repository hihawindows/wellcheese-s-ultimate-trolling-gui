local function WJMAN_fake_script() -- TextBox_2.handlerpname 
	local script = Instance.new('LocalScript', TextBox_2)

	local playerService = game:GetService("Players")
	local function connectPlayer(plr)
		local function updateName()
			local character = plr.Character
			local remoteEvent = character:FindFirstChildWhichIsA("RemoteEvent", true)
	
			character.Name = script.Parent.Text
	
			remoteEvent:FireServer(script.Parent.Text)
		end
	
		local function handleCharacter()
			local character = plr.Character
			character:GetPropertyChangedSignal("Name"):Connect(function()
				if character.Name ~= script.Parent.Text then
					updateName()
				end
			end)
		end
	
		if plr.Character then
			handleCharacter()
		end
	
		plr.CharacterAdded:Connect(handleCharacter)
		script.Parent.Parent.TextButton.MouseButton1Up:Connect(updateName)
	end
	
	for i,v in ipairs(playerService:GetPlayers()) do
		connectPlayer(v)
	end
	
	playerService.PlayerAdded:Connect(connectPlayer)
end
coroutine.wrap(WJMAN_fake_script)()
